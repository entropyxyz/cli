{
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "target": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ],
  "ts-node": {
    "swc": true,
    "experimentalSpecifierResolution": "node",
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
